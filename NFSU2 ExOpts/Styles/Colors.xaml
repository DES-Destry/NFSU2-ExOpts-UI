<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:NFSU2_ExOpts.Styles">

    <SolidColorBrush x:Key="BlackBrush" Color="#000000"/>
    <Color x:Key="BlackColor">#000000</Color>

    <SolidColorBrush x:Key="ActiveBgBrush" Color="#1D1F20"/>
    <Color x:Key="ActiveBgColor">#1D1F20</Color>

    <SolidColorBrush x:Key="StaticBgBrush" Color="#181A1B"/>
    <Color x:Key="StaticBgColor">#181A1B</Color>

    <SolidColorBrush x:Key="MainBrush" Color="#00C953"/>
    <Color x:Key="MainColor">#00C953</Color>

    <SolidColorBrush x:Key="WhiteBrush" Color="#FFFFF0"/>
    <Color x:Key="WhiteColor">#FFFFF0</Color>

    <SolidColorBrush x:Key="RedBrush" Color="#FF0000"/>
    <Color x:Key="RedColor">#FF0000</Color>

</ResourceDictionary>